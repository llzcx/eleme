<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cx.springboot02.mapper.ManagerMapper">


    <select id="checkManage" resultType="com.cx.springboot02.pojo.Manager">
        select *
        from takeoutweb.manager
        where name= #{name} and password = #{password}
    </select>
</mapper>
